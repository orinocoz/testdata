$TTL 86400
@   IN  SOA {{ startup_name }}. root.{{ startup_name }}. (
            2   ; Serial
       604800   ; Refresh
        86400   ; Retry
      2419200   ; Expire
       604800 ) ; Negative Cache TTL
;
@   IN  NS  {{ startup_name }}.
@   IN  A   {{ hostvars[inventory_hostname]['ansible_default_ipv4']['address'] }}
